  "description": "A cyber-biological software toy that allows you to create various ecosystems using living cells",
  "author": "David Davtyan <david-d25@protonmail.com>",
  "homepage": "https://david-d25.github.io/cybercells",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "prebuild": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "core-js": "~3.26.1",
    "gl-matrix": "^3.4.3",
    "vue": "~3.2.45",
    "vue-class-component": "^8.0.0-0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "^5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "babel-loader": "^8.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "gh-pages": "^4.0.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "typescript": "~4.5.5",
    "vue-loader": "^16.8.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "vue/setup-compiler-macros": true
